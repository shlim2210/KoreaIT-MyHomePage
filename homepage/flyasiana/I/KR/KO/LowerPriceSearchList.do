		//달력 그리기
		var GRID_INQUIRYCALENDAR = function(date, data){

			var daygrid = {};

			daygrid.wrapper = function(){return $('.daygrid_wrap')};

			// 년도, 월 settings
			daygrid.setYear = date.substring(0, 4);
			daygrid.setMon = date.substring(4, 6);

			// 가격 데이타
			daygrid.price_data = [];

			// cff 데이타
			daygrid.cff_data = [];

			// 화면 비활성화 날짜
			daygrid.disabled_day = [];

			// 최저가 표시 날짜
			daygrid.low_price_day = [];

			// 매진 표시 날짜
			daygrid.soldout_day = [];

			// 가격 데이타, cff, 최저가 표시 설정
			var lowIdx = 0;
			for(var i=0; i<data.RESULT_DATA.INS_DATA.instantSearchInfoList.length; i++){
				var day = data.RESULT_DATA.INS_DATA.instantSearchInfoList[i].departureDate.substring(6, 8);
				if(day < 10){
					day = day.replace('0', '');
				}
				
				if(data.RESULT_DATA.INS_DATA.instantSearchInfoList[i].totalAmount) {
					daygrid.price_data[i] = data.RESULT_DATA.INS_DATA.instantSearchInfoList[i].totalAmount.setComma();
				}
				
				daygrid.cff_data[i] = data.RESULT_DATA.INS_DATA.instantSearchInfoList[i].cff;
				if(data.RESULT_DATA.INS_DATA.instantSearchInfoList[i].totalAmount == data.RESULT_DATA.LOW_PRICE){
					daygrid.low_price_day[lowIdx] = day;
					lowIdx++;
				}
			}
			
			// 비활성화 날짜 체크
			var disIdx = 0;
			for(var i=1; i<=31; i++) {
				for(var j=0; j<data.RESULT_DATA.INS_DATA.instantSearchInfoList.length; j++){
					var chk_day = data.RESULT_DATA.INS_DATA.instantSearchInfoList[j].departureDate.substring(6, 8);
					if(chk_day < 10){
						chk_day = chk_day.replace('0', '');
					}
					if(i == chk_day){
						break;
					}
					if((j+1) == data.RESULT_DATA.INS_DATA.instantSearchInfoList.length && i <= data.RESULT_DATA.INS_DATA.instantSearchInfoList.length){
						daygrid.disabled_day[disIdx] = chk_day;
