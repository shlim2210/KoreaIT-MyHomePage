				arrCalendar.push('<div class="daygrid_mask">');
				var oStartDt = new Date(daygrid.dateValue.getTime());
		        // 1일에서 1일의 요일을 빼면 그 주 첫번째 날이 나옴
				oStartDt.setDate(oStartDt.getDate() - oStartDt.getDay());

				var searchTripType = $('#searchTripType').val();

				for(var i=0; i<50; i++) {
					if(i % 7 == 0) {
						arrCalendar.push('<div class="daygrid_slider" id="slideSub_' + i + '">');
					}

					// blank (현재 달이 아닌 날짜)
					var notCurMonth = daygrid.dateValue.getMonth() != oStartDt.getMonth() ? 'not-this-month ' : '';

					if( $('#span_cabin_class').attr('data-seatClass') == 'economy' ){
					    arrCalendar.push('<div class="daygrid_date econ ' + notCurMonth + '">');
					}else {
					    arrCalendar.push('<div class="daygrid_date busi ' + notCurMonth + '">');
					}

					if(notCurMonth == "") {
						//[KWCAG] 웹접근성 수정 해당 요일 정보 추가 / 180222 ksh
						arrCalendar.push('<div class="txt_date" data-date="' + oStartDt.getDate() + '">' + oStartDt.getDate() + '<span class="hidden" id=\"arrWeek_txt_'+oStartDt.getDate()+'\">'+ arrWeek[i%7] +'</span>' + '</div>');
						arrCalendar.push('<input type="hidden" id="hid_cff' + oStartDt.getDate() + '" name="hid_cff' + oStartDt.getDate() + '" value="">');
						arrCalendar.push('<input type="radio" id="sDate' + oStartDt.getDate() + '" name="sel_date" value="">');
						arrCalendar.push("<label for=\"sDate" + oStartDt.getDate() + "\"></label>");
					}

					arrCalendar.push('</div>');
					oStartDt.setDate(oStartDt.getDate() + 1);

					if(i % 7 == 6) {
						arrCalendar.push('</div>');

						/* 상세내용출력 영역 */
						arrCalendar.push('<div class="traveldetails_row detailSub_' + (i-6) + '">');
						arrCalendar.push('<div class="fl">');
						arrCalendar.push('<div class="location">');
						arrCalendar.push('<span></span>');
						arrCalendar.push('<span></span>');
						arrCalendar.push('</div>');

						if(searchTripType == 'RT'){
							arrCalendar.push("<div class=\"txt_date\"><span></span> ~ <span></span> <span></span>일</div>"); //일
						}else {
							arrCalendar.push('<div class="txt_date"><span></span></div>');
						}

						arrCalendar.push('</div>');
						arrCalendar.push('<div class="fr">');
						arrCalendar.push('<ul>');
						arrCalendar.push("<li>탑승인원 : <span></span></li>"); //탑승인원
						arrCalendar.push("<li>클래스 : <span></span></li>"); //클래스
						arrCalendar.push('</ul>');
						arrCalendar.push("<button type=\"button\" id=\"btn_regist_travel\" class=\"btn_M white\">항공권 예약</button>"); //항공권 예약
						arrCalendar.push('</div>');
						arrCalendar.push('</div>');

						if(daygrid.dateValue.getMonth() != oStartDt.getMonth()) break;
					}
				}
				arrCalendar.push('</div></div>');

				$('#calendar_monthGrid').html(arrCalendar.join(""));

				daygrid.changeMonth();
			};

			/* 날짜 선택시 */
			daygrid.selectDay = function() {
				daygrid.wrapper().find('input[type="radio"]').unbind().on('change', function(){
					daygrid.wrapper().find('.daygrid_date').removeClass('on');
					$(this).parent('.daygrid_date').addClass('on');
					$('.daygrid_slider').removeClass('on');
					$(this).parents('.daygrid_slider').addClass('on');

					var oStartDt = new Date(daygrid.dateValue.getTime());
					var arrWeek = "일,월,화,수,목,금,토".split(","); //일,월,화,수,목,금,토

					var depDate = $(this).attr('id').replace('sDate', '');
					oStartDt.setFullYear(daygrid.setYear);
					oStartDt.setMonth(parseInt(daygrid.setMon)-1);
					oStartDt.setDate(depDate);

					var dep_temp_month = oStartDt.getMonth()+1;
					if(dep_temp_month < 10){
						dep_temp_month = '0'+dep_temp_month;
					}
					var dep_temp_date = oStartDt.getDate();
					if(dep_temp_date < 10){
						dep_temp_date = '0'+dep_temp_date;
					}
					var dep_temp_day = arrWeek[oStartDt.getDay()];
					var dep_date_txt = oStartDt.getFullYear()+'.'+dep_temp_month+'.'+dep_temp_date+'('+dep_temp_day+')';
					var dep_date_r = oStartDt.getFullYear()+''+dep_temp_month+''+dep_temp_date;
					$('#DEP_DATE_R').val(dep_date_r);

					oStartDt.setDate(parseInt(depDate) + parseInt($('#STAY_DAY_R').val()));
					var ret_temp_month = oStartDt.getMonth()+1;
					if(ret_temp_month < 10){
						ret_temp_month = '0'+ret_temp_month;
					}
					var ret_temp_date = oStartDt.getDate();
					if(ret_temp_date < 10){
						ret_temp_date = '0'+ret_temp_date;
					}
					var ret_temp_day = arrWeek[oStartDt.getDay()];
					var ret_date_txt = oStartDt.getFullYear()+'.'+ret_temp_month+'.'+ret_temp_date+'('+ret_temp_day+')';
					var ret_date_r = oStartDt.getFullYear()+''+ret_temp_month+''+ret_temp_date;
					$('#RET_DATE_R').val(ret_date_r);

					daygrid.setDetailInfo(dep_date_txt, ret_date_txt);
					daygrid.showDayDetail();
				});
			}

		    /* Month 선택 상단에 현재 년 월 다시 표시 */
			daygrid.changeMonth = function() {
				daygrid.setAfterData();
			}

		    /* 날짜 객체를 년 월 문자 형식으로 변환 */
			daygrid.getYearMonth = function(oDate) {
				return oDate.getFullYear() + "년 " + (oDate.getMonth() + 1) + "월"; //년, 월
			}

			daygrid.setDetailInfo = function(dep_date_txt, ret_date_txt){
				var searchTripType = $('#searchTripType').val();
				var objDetail = $(".traveldetails_row");
				objDetail.find(".fl .location span:eq(0)").text(daygrid.dayDetailInfo.startPosition);
				objDetail.find(".fl .location span:eq(1)").text(daygrid.dayDetailInfo.endPosition);
				objDetail.find(".fl .txt_date span:eq(0)").text(dep_date_txt);

				if(searchTripType == 'RT'){
					objDetail.find(".fl .txt_date span:eq(1)").text(ret_date_txt);
					objDetail.find(".fl .txt_date span:eq(2)").text(daygrid.dayDetailInfo.totalDate);
				}

				objDetail.find(".fr ul li:eq(0) span").text(daygrid.dayDetailInfo.cntPerson);
				objDetail.find(".fr ul li:eq(1) span").text(daygrid.dayDetailInfo.seatClass);
			}

			// 달력 그려준 후 데이터 입력
			daygrid.setAfterData = function(){
				// 전체 날짜 loop
				daygrid.allDayCheck();

				// 특정 날짜 비활성화
				daygrid.setDay_disabled();

				// 최저가 라벨 표시
				daygrid.setDay_cheapDay();

				// 매진 날짜 표시
				daygrid.setDay_soldOut();

				// 날짜(라디오버튼) 선택시 동작 함수
				daygrid.selectDay();
			}

			daygrid.init = function(){
				daygrid.destroy();
				daygrid.create();
			};

			daygrid.init();

			return daygrid;
		};




