		/*
		* json - bookConditionData 를 생성한다.
		* 00. bizType : REV / RED
		* */
		var makeBookConditionData = function makeBookConditionData(bizType) {

			var domIntType = '';
			if($('#DEP_AREA_R').val() == 'KR' && $('#ARR_AREA_R').val() == 'KR') {
				domIntType = 'D';
			}else{
				domIntType = 'I';
			}

			var bookConditionData = new bookConditionDataObject();
			bookConditionData.bizType					= bizType;
			bookConditionData.tripType					= $('#TRIP_TYPE_R').val();
			bookConditionData.domIntType				= domIntType;
			bookConditionData.eventCFF					= $('[name="sel_date"]:checked').val();

			var segmentConditionDatas = [];

			var segmentData1 = new segmentConditionDatasObject();
			segmentData1.departureArea			= $('#DEP_AREA_R').val();
			segmentData1.departureAirport		= $('#DEP_AIRPORT_R').val();
			segmentData1.departureAirportName	= $('#DEP_AIRPORT_DESC_R').val();
			segmentData1.departureCity			= $('#DEP_CITY_R').val();
			segmentData1.departureCityName		= $('#DEP_CITY_NAME_R').val();
			segmentData1.departureDateTime		= $('#DEP_DATE_R').val() + '0000';
			segmentData1.arrivalArea			= $('#ARR_AREA_R').val();
			segmentData1.arrivalAirport			= $('#ARR_AIRPORT_R').val();
			segmentData1.arrivalAirportName		= $('#ARR_AIRPORT_DESC_R').val();
			segmentData1.arrivalCity			= $('#ARR_CITY_R').val();
			segmentData1.arrivalCityName		= $('#ARR_CITY_NAME_R').val();
			segmentData1.beforeArrivalDate		= $('#STAY_DAY_R').val();

			var selectCabinClass = $('#CABIN_CLASS_R').val();

			if(bookConditionData.tripType == 'RT') {

				if(selectCabinClass == 'E'){
					segmentData1.cabinClassList			= ["R","E"];
				}else if(selectCabinClass == 'B'){
					segmentData1.cabinClassList			= ["B"];
				}

				segmentConditionDatas.push(segmentData1);

				var segmentData2 = new segmentConditionDatasObject();
				segmentData2.departureArea			= $('#ARR_AREA_R').val();
				segmentData2.departureAirport		= $('#ARR_AIRPORT_R').val();
				segmentData2.departureAirportName	= $('#ARR_AIRPORT_DESC_R').val();
				segmentData2.departureCity			= $('#ARR_CITY_R').val();
				segmentData2.departureCityName		= $('#ARR_CITY_NAME_R').val();
				segmentData2.departureDateTime		= $('#RET_DATE_R').val() + '0000';
				segmentData2.arrivalArea			= $('#DEP_AREA_R').val();
				segmentData2.arrivalAirport			= $('#DEP_AIRPORT_R').val();
				segmentData2.arrivalAirportName		= $('#DEP_AIRPORT_DESC_R').val();
				segmentData2.arrivalCity			= $('#DEP_CITY_R').val();
				segmentData2.arrivalCityName		= $('#DEP_CITY_NAME_R').val();

				if(selectCabinClass == 'E'){
					segmentData2.cabinClassList			= ["R","E"];
				}else if(selectCabinClass == 'B'){
					segmentData2.cabinClassList			= ["B"];
				}

				segmentConditionDatas.push(segmentData2);

			}else if(bookConditionData.tripType == 'OW') {

				if(selectCabinClass == 'E'){
					segmentData1.cabinClassList			= ["R","E"];
				}else if(selectCabinClass == 'B'){
					segmentData1.cabinClassList			= ["B"];
				}

				segmentConditionDatas.push(segmentData1);
			}
			bookConditionData.segmentConditionDatas		= segmentConditionDatas;

			var passengerConditionDatas	= [];
			var	adultCount = 1;

			if(adultCount > 0) {
				for(var i=0; i<adultCount; i++) {
					var passengerConditionData = new passengerConditionDatasObject();
					passengerConditionData.passengerType = 'ADT';
					passengerConditionData.passengerTypeDesc = "성인"; //성인
					passengerConditionDatas.push(passengerConditionData);
				}
			}

			bookConditionData.passengerConditionDatas 		= passengerConditionDatas;

			var searchCurrency;
			if($('#DEP_AREA_R').val() == 'CN' && bizType != "RED") {
				searchCurrency = 'CNY';
			}else{
				searchCurrency = '';
			}

			bookConditionData.searchCurrency			= searchCurrency;
			bookConditionData.salesType					= 'lowerPrice';

			$('#bookConditionData').val(JSON.stringify(bookConditionData));
		};

